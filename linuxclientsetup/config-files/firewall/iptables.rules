*filter
:INPUT DROP [0:0]
:FORWARD DROP [0:0]
:OUTPUT ACCEPT [0:0]
:LOGDROP - [0:0]
:LOGREJECT - [0:0]
:karoshi-auto - [0:0]
:karoshi-domain - [0:0]
:karoshi-local - [0:0]
-A INPUT -i lo -j ACCEPT
-A INPUT -m conntrack --ctstate RELATED,ESTABLISHED -j ACCEPT
-A INPUT -j karoshi-local
-A INPUT -j karoshi-domain
-A INPUT -j karoshi-auto
-A LOGDROP -j LOG
-A LOGDROP -j DROP
-A LOGREJECT -j LOG
-A LOGREJECT -j REJECT --reject-with icmp-port-unreachable
COMMIT
